     function getInternetExplorerVersion_987859() {
         // Returns the version of Internet Explorer or a -1
         // (indicating the use of another browser).

             var rv = -1; // Return value assumes failure.
             if (navigator.appName == 'Microsoft Internet Explorer') {
                 isIEBrowser_987859=true;
                 var ua = navigator.userAgent;
                 var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");

                 if (re.exec(ua) != null)
                     rv = parseFloat( RegExp.$1 );
             }

             return rv;
         }

      //returns true if ie version is less than 9, say ie6, ie7, ie8
         // -1 for non IE browsers.
         function isIEBrowserWithVersionLessThan9_987859 () {

             browserVersion_987859 = getInternetExplorerVersion_987859();  //-1 for non IE browsers
             if((browserVersion_987859 != -1) && (browserVersion_987859 < 9)) {
                 return true;

             }
             return false;
         }

    //code to detect Edge Features, courtesy  (http://dl.dropboxusercontent.com/u/13483458/test-edge.html)
    var testEle_987859=document.createElement("div_987859");
    function isSupported_987859(a){

        var d=testEle_987859.style,e;
        for(i=0;i<a.length;i++)
            if(e=a[i],d[e]!==void 0)
                return!0;
        return!1
    }

    function supportsRGBA_987859(){

        testEle_987859.cssText="background-color:rgba(150,255,150,.5)";
        if((""+testEle_987859.style.backgroundColor).indexOf("rgba")==0)
            return!0;
        return!1
    }

    var hasTransform_987859=isSupported_987859([
        "transformProperty",
        "WebkitTransform",
        "MozTransform",
        "OTransform",
        "msTransform"
    ]),

    hasSVG_987859=!!document.createElementNS&&!!document.createElementNS("http://www.w3.org/2000/svg","svg").createSVGRect,
    hasRGBA_987859=supportsRGBA_987859(),
    hasJSON_987859=window.JSON&&window.JSON.parse&&window.JSON.stringify,
    readyToPlay=!1;

    function isIEBrowserVersion9_987859() {
        return (isIEBrowser_987859 && (browserVersion_987859 == 9)) ? true : false;
    }

    function isEdgeSupported_987859() {
        if(isIEBrowserVersion9_987859()) {
            return "y";           //hardcoding IE9 edge support.
        }
        if(hasTransform_987859) {
            if(requiresSVG_987859&&!hasSVG_987859)
                return "f";
            return "y";
        }
        return "f";
    }

    function isCanvasSupported_987859(){
      var elem = document.createElement('canvas');
      return !!(elem.getContext && elem.getContext('2d'));
    }

    function isHTML5FeaturesSupported_987859() {
         return (isCanvasSupported_987859()) ? "y" : "f";
    }

    var requiresSVG_987859=false;
